#install.packages("ggplot2")
#install.packages("TSstudio")
library(ggplot2)
# library(TSstudio)
plutil::pluglibrary(TSstudio)

list.files()
TagNum <- "dz3_first_track_dist"
first_track<-read.csv(paste(TagNum,".csv",sep=""), sep=";", na.strings = "#ЗНАЧ!",stringsAsFactors=F)

first_track$type<-as.POSIXlt.character(first_track$type,format="%H:%M:%S")
первый_круг<-first_track
ts_plot(первый_круг)

TagNum <- "dz3_second_track_dist"
second_track<-read.csv(paste(TagNum,".csv",sep=""), sep=";", na.strings = "#ЗНАЧ!",stringsAsFactors=F)
second_track$type<-as.POSIXlt.character(second_track$type,format="%H:%M:%S")



второй_круг<-second_track
ts_plot(второй_круг)

TagNum <- "высота локаций"
high<-read.csv(paste(TagNum,".csv",sep=""), sep=";", na.strings = "#ЗНАЧ!",stringsAsFactors=F)
high<-high[high$dis<50,]

ggplot(high, aes(x=high, y=dist)) + 
  geom_point()+
  geom_smooth(method=lm, se= FALSE , col='red', size= 0.5 ) +
  theme(text=element_text(size= 30,face = "bold")) +
  labs(       x = "Высота, м", 
              y = "Дистанция, м")

